# ========================================
# CCTV Search Backend - Environment Variables
# ========================================
# Copy this file to .env and fill in your actual values

# ----------------------------------------
# Qdrant Vector Database Configuration
# ----------------------------------------
QDRANT_URL=http://localhost:6334
QDRANT_API_KEY=my-secret-api-key
COLLECTION_NAME=nt-cctv-vehicles

# ----------------------------------------
# AI Image Embedding Service Configuration
# ----------------------------------------
# URL of your Python AI service for generating image embeddings
AI_SERVICE_URL=http://localhost:5090

# ----------------------------------------
# CCTV API Configuration
# ----------------------------------------
# External CCTV metadata API endpoint
CCTV_API_URL=https://ntvideo.totbb.net/video-metadata/train-data-condition

# REQUIRED: Authentication token for CCTV API
# Get this from your CCTV API provider
CCTV_AUTH_TOKEN=your_cctv_auth_token_here

# CCTV camera identifier
CCTV_ID=cctv01
